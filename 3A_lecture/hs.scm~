(define make-vect cons)
(define x-coord car)
(define y-coord cdr)

(define make-seg cons)
(define start car)
(define end cdr)

(define (make-rect origin horizontal vertical)
  (list origin horizontal vertical))

(define orig car)
(define horiz (lambda(i) (car (cdr i))))
(define vert (lambda(i) (car (cdr (cdr i)))))
